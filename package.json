{
  "name": "vscode-fold",
  "displayName": "Fold",
  "description": "Visual Studio Code extension for automatic folding üÉè.",
  "version": "1.0.0",
  "repository": "github:felicio/vscode-fold",
  "publisher": "felicio",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.3.0"
  },
  "categories": [
    "Formatters",
    "Other"
  ],
  "qna": false,
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "configuration": {
      "title": "Fold Configuration",
      "properties": {
        "fold.level": {
          "type": "number",
          "default": 1,
          "description": "Default fold level."
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "vscode": "^1.1.6"
  }
}
